<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fengze.shop.mapper.MenuDoMapper">
    <resultMap id="BaseResultMap" type="com.fengze.shop.model.MenuDo">
        <!--
          WARNING - @mbg.generated
        -->
        <id column="TYPE_ID" property="typeId" jdbcType="BIGINT"/>
        <result column="TYPE_NAME" property="typeName" jdbcType="VARCHAR"/>
        <result column="TYPE_P_ID" property="typePId" jdbcType="BIGINT"/>
        <result column="MENU_URI" property="menuUri" jdbcType="VARCHAR"/>
        <result column="MENU_STYLE" property="menuStyle" jdbcType="TINYINT"/>
        <result column="IS_MENU" property="isMenu" jdbcType="TINYINT"/>
        <result column="STATE" property="state" jdbcType="TINYINT"/>
        <result column="CREATE_BY" property="createBy" jdbcType="BIGINT"/>
        <result column="CREATE_DATE" property="createDate" jdbcType="TIMESTAMP"/>
        <result column="UPDATE_BY" property="updateBy" jdbcType="BIGINT"/>
        <result column="UPDATE_DATE" property="updateDate" jdbcType="TIMESTAMP"/>
    </resultMap>

    <select id="queryLeafNode" resultType="Long">
        SELECT
        A.type_id
        FROM tb_pro_type A
        WHERE NOT EXISTS
        (SELECT * FROM tb_pro_type T1 ,
        tb_pro_type T2 WHERE T1.type_id=T2.type_p_id AND T1.type_id=A.type_id)

    </select>
</mapper>